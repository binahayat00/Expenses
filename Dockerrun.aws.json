{
    "AWSEBDockerrunVersion": "1",
    "Image": {
        "Name": "docker/Dockerfile",
        "Update": "true"
    },
    "Ports": [
        {
            "ContainerPort": "80"
        }
    ],
    "Environment": [
        {
            "Name": "USER",
            "Value": "ec2-user"
        },
        {
            "Name": "USER_ID",
            "Value": "1000"
        },
        {
            "Name": "GROUP_ID",
            "Value": "1000"
        }
    ]
}